<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ahad.share_cab">

    <application
        android:name="com.ahad.share_cab.sharecab"
        android:allowBackup="true"
        android:appComponentFactory="whateverString"
        android:icon="@mipmap/icon_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_main_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:appComponentFactory">
        <activity android:name="com.ahad.share_cab.about_activity" />
        <activity android:name="com.ahad.share_cab.recent_chat_activity" />
        <activity android:name="com.ahad.share_cab.history_activity" />
        <activity android:name="com.ahad.share_cab.chat_activity" />
        <activity android:name="com.ahad.share_cab.activity3" />
        <activity android:name="com.ahad.share_cab.activity2" />
        <activity android:name="com.ahad.share_cab.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>